<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.akpinar.acilnumaralar.MainActivity">

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="left|top">

        <TextView
            android:layout_width="102dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="ACİL"
            android:id="@+id/textView"
            android:layout_row="0"
            android:layout_column="0"
            android:gravity="center_vertical"
            android:layout_gravity="center_horizontal"
            android:layout_columnSpan="3"
            android:textColor="#df0303" />

        <Button
            android:layout_width="133dp"
            android:layout_height="96dp"
            android:id="@+id/ambulans"
            android:layout_row="1"
            android:layout_column="0"
            android:gravity="center|top"
            android:background="@drawable/ambulans"
            android:foreground="@android:drawable/stat_sys_phone_call"
            android:foregroundGravity="center_horizontal|bottom" />

        <Button
            android:layout_width="133dp"
            android:layout_height="96dp"
            android:id="@+id/polis"
            android:layout_row="1"
            android:layout_column="1"
            android:gravity="center|top"
            android:background="@drawable/polis"
            android:foregroundGravity="center_horizontal|bottom"
            android:foreground="@android:drawable/stat_sys_phone_call" />

        <Button
            android:layout_width="133dp"
            android:layout_height="96dp"
            android:id="@+id/iftaiye"
            android:layout_row="1"
            android:layout_column="2"
            android:gravity="center|top"
            android:background="@drawable/itfaiye"
            android:foreground="@android:drawable/stat_sys_phone_call"
            android:foregroundGravity="center_horizontal|bottom" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="POLİS"
            android:id="@+id/textView5"
            android:layout_row="2"
            android:layout_column="1"
            android:gravity="center"
            android:layout_gravity="center_horizontal"
            android:textColor="#0008e3" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="İTFAİYE"
            android:id="@+id/textView6"
            android:layout_row="2"
            android:layout_column="2"
            android:gravity="center"
            android:layout_gravity="center_horizontal|bottom|left"
            android:textColor="#f10000" />

        <Button
            android:layout_width="133dp"
            android:layout_height="96dp"
            android:id="@+id/ormanyangini"
            android:layout_row="3"
            android:layout_column="0"
            android:gravity="center|top"
            android:background="@drawable/ormanyangini"
            android:foreground="@android:drawable/stat_sys_phone_call"
            android:foregroundGravity="center_horizontal|bottom" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="AMBULANS"
            android:id="@+id/textView4"
            android:layout_row="2"
            android:layout_column="0"
            android:gravity="center"
            android:layout_gravity="center_horizontal"
            android:textColor="#e41d1d" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="ORMANYANGIN"
            android:id="@+id/textView7"
            android:layout_row="4"
            android:layout_column="0"
            android:gravity="center"
            android:layout_gravity="center_horizontal"
            android:textColor="#fb0000" />

        <Button
            android:layout_width="133dp"
            android:layout_height="96dp"
            android:id="@+id/dogalgazacil"
            android:layout_row="3"
            android:layout_column="1"
            android:gravity="center|top"
            android:background="@drawable/dogalgaz"
            android:foreground="@android:drawable/stat_sys_phone_call"
            android:foregroundGravity="center_horizontal|bottom" />

        <Button
            android:layout_width="133dp"
            android:layout_height="96dp"
            android:id="@+id/jandarma"
            android:layout_row="3"
            android:layout_column="2"
            android:gravity="center|top"
            android:background="@drawable/jandarma"
            android:foreground="@android:drawable/stat_sys_phone_call"
            android:foregroundGravity="center_horizontal|bottom" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView10"
            android:layout_row="5"
            android:layout_column="0" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="DOĞALGAZ"
            android:id="@+id/textView8"
            android:layout_row="4"
            android:layout_column="1"
            android:layout_gravity="center_horizontal"
            android:textColor="#34c1b5" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="JANDARMA"
            android:id="@+id/textView9"
            android:layout_row="4"
            android:layout_column="2"
            android:gravity="center"
            android:layout_gravity="center_horizontal|left"
            android:textColor="#0047ec" />

        <TextView
            android:layout_width="122dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="SAĞLIK"
            android:id="@+id/textView2"
            android:layout_row="6"
            android:layout_column="0"
            android:layout_gravity="center_horizontal"
            android:layout_columnSpan="3"
            android:textColor="@android:color/holo_green_dark"
            android:autoText="true" />

        <Button
            android:layout_width="133dp"
            android:layout_height="96dp"
            android:text="ZEHİR DANIŞMA"
            android:id="@+id/zehirdanisma"
            android:layout_row="7"
            android:layout_column="0"
            android:gravity="center|top"
            android:foreground="@android:drawable/stat_sys_phone_call"
            android:foregroundGravity="center_horizontal|bottom" />

        <Button
            android:layout_width="133dp"
            android:layout_height="96dp"
            android:text="KIZILAY"
            android:id="@+id/kizilay"
            android:layout_row="7"
            android:layout_column="1"
            android:gravity="center|top"
            android:background="@drawable/kizilay"
            android:foreground="@android:drawable/stat_sys_phone_call"
            android:foregroundGravity="center_horizontal|bottom" />

        <Button
            android:layout_width="133dp"
            android:layout_height="96dp"
            android:text="SAĞLIK DANIŞMA"
            android:id="@+id/saglikdanisma"
            android:layout_row="7"
            android:layout_column="2"
            android:gravity="center|top"
            android:foregroundGravity="center_horizontal|bottom"
            android:foreground="@android:drawable/stat_sys_phone_call" />

        <Button
            android:layout_width="133dp"
            android:layout_height="96dp"
            android:text="RUHSAL BUNALIM"
            android:id="@+id/ruhsalbunalim"
            android:layout_row="9"
            android:layout_column="0"
            android:gravity="center|top"
            android:foregroundGravity="center_horizontal|bottom"
            android:foreground="@android:drawable/stat_sys_phone_call" />

        <Button
            android:layout_width="133dp"
            android:layout_height="96dp"
            android:text="UYUŞTURUCU BİLGİ"
            android:id="@+id/uyusturucubilgi"
            android:layout_row="9"
            android:layout_column="1"
            android:gravity="center|top"
            android:foregroundGravity="center|center_horizontal|bottom"
            android:foreground="@android:drawable/stat_sys_phone_call" />

        <Button
            android:layout_width="133dp"
            android:layout_height="96dp"
            android:text="ALO DOKTORUM YANIMDA"
            android:id="@+id/alodoktorumyanimda"
            android:layout_row="9"
            android:layout_column="2"
            android:gravity="center|top"
            android:foregroundGravity="center_horizontal|bottom"
            android:foreground="@android:drawable/stat_sys_phone_call" />

        <TextView
            android:layout_width="396dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="ARIZALAR VE KURUMLAR"
            android:id="@+id/textView3"
            android:layout_row="11"
            android:layout_column="0"
            android:layout_columnSpan="3"
            android:gravity="center|center_horizontal"
            android:background="#26ccff" />

        <Button
            android:layout_width="133dp"
            android:layout_height="70dp"
            android:text="ELEKTRİK ARIZA"
            android:id="@+id/elektrikariza"
            android:layout_row="12"
            android:layout_column="0"
            android:gravity="center|top"
            android:backgroundTintMode="src_atop"
            android:foreground="@android:drawable/stat_sys_phone_call"
            android:foregroundGravity="center_horizontal|bottom" />

        <Button
            android:layout_width="133dp"
            android:layout_height="68dp"
            android:text="SU ARIZA"
            android:id="@+id/suariza"
            android:layout_row="12"
            android:layout_column="1"
            android:gravity="center|top"
            android:backgroundTintMode="src_atop"
            android:foreground="@android:drawable/stat_sys_phone_call"
            android:foregroundGravity="center_vertical|center_horizontal|bottom" />

        <Button
            android:layout_width="133dp"
            android:layout_height="71dp"
            android:text="TELEFON ARIZA"
            android:id="@+id/telefonariza"
            android:layout_row="12"
            android:layout_column="2"
            android:gravity="center|top"
            android:backgroundTintMode="src_atop"
            android:foreground="@android:drawable/stat_sys_phone_call"
            android:foregroundGravity="center_horizontal|bottom" />

        <Button
            android:layout_width="133dp"
            android:layout_height="69dp"
            android:text="CENAZE HİZMETLERİ"
            android:id="@+id/cenazehizmetleri"
            android:layout_row="13"
            android:layout_column="0"
            android:gravity="center|top"
            android:backgroundTintMode="src_atop"
            android:foreground="@android:drawable/stat_sys_phone_call"
            android:foregroundGravity="center_horizontal|bottom" />

        <Button
            android:layout_width="133dp"
            android:layout_height="68dp"
            android:text="İŞKUR"
            android:id="@+id/iskur"
            android:layout_row="13"
            android:layout_column="1"
            android:gravity="center|top"
            android:backgroundTintMode="src_atop"
            android:foreground="@android:drawable/stat_sys_phone_call"
            android:foregroundGravity="center_horizontal|bottom" />

        <Button
            android:layout_width="133dp"
            android:layout_height="74dp"
            android:text="VERGİ DANIŞMA"
            android:id="@+id/vergidanisma"
            android:layout_row="13"
            android:layout_column="2"
            android:gravity="center|top"
            android:backgroundTintMode="src_atop"
            android:foreground="@android:drawable/stat_sys_phone_call"
            android:foregroundGravity="center_horizontal|bottom" />

    </GridLayout>
</FrameLayout>
